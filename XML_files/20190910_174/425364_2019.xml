.</P></SHORT_DESCR></OBJECT_CONTRACT><COMPLEMENTARY_INFO><DATE_DISPATCH_NOTICE>2019-09-05</DATE_DISPATCH_NOTICE><NOTICE_NUMBER_OJ>2019/S 132-325692</NOTICE_NUMBER_OJ></COMPLEMENTARY_INFO><CHANGES><CHANGE PUBLICATION="YES"><WHERE><SECTION>II.2.7)</SECTION><LOT_NO>Start</LOT_NO><LABEL>Duration of the contract or the framework agreement</LABEL></WHERE><OLD_VALUE><DATE>2020-01-01</DATE></OLD_VALUE><NEW_VALUE><DATE>2020-02-01</DATE></NEW_VALUE></CHANGE><CHANGE PUBLICATION="YES"><WHERE><SECTION>II.2.7</SECTION><LOT_NO>End</LOT_NO><LABEL>Duration of the contract or the framework agreement</LABEL></WHERE><OLD_VALUE><DATE>2023-12-31</DATE></OLD_VALUE><NEW_VALUE><DATE>2024-01-31</DATE></NEW_VALUE></CHANGE></CHANGES></F14_2014></FORM_SECTION></TED_EXPORT>